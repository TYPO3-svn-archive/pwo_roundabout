<T3DataStructure>
	<meta>
	<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<ratype>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_source</label>
						<onChange>reload</onChange>
						<config>
							<type>select</type>
							<items type="array">
							  <numIndex index="0" type="array">
								<numIndex index="0">LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.value_folder</numIndex>
								<numIndex index="1">folder</numIndex>
							  </numIndex>
							  <numIndex index="1" type="array">
								<numIndex index="0">LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.value_content</numIndex>
								<numIndex index="1">tt_content</numIndex>
							  </numIndex>
							</items>
						</config>
						</TCEforms>
					</ratype>
					<path>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_path</label>
						<displayCond>FIELD:ratype:!=:tt_content</displayCond>
						<config>
							<type>input</type>
							<size>30</size>
						</config>
						</TCEforms>
					</path>
					<tt_contents>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_content</label>
						<displayCond>FIELD:ratype:=:tt_content</displayCond>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>tt_content</allowed>
							<size>5</size>
							<maxitems>20</maxitems>
							<minitems>0</minitems>
						</config>
						</TCEforms>
					</tt_contents>
					<width>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_width</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>400</default>
						</config>
						</TCEforms>
					</width>					
					<height>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_height</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>400</default>
						</config>
						</TCEforms>
					</height>
					<templateFile>
						<TCEforms>
							<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_template</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard&amp;act=file</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
											<allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
										</params>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</templateFile>
				</el>
			</ROOT>
		</sDEF>
		<s_roundabout>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<bearing>
						<TCEforms>
						<label>bearing:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0.0</default>
						</config>
						</TCEforms>
					</bearing>
					<tilt>
						<TCEforms>
						<label>tilt:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0.0</default>
						</config>
						</TCEforms>
					</tilt>
					<minZ>
						<TCEforms>
						<label>minZ:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>100</default>
						</config>
						</TCEforms>
					</minZ>
					<maxZ>
						<TCEforms>
						<label>maxZ:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>280</default>
						</config>
						</TCEforms>
					</maxZ>
					<minOpacity>
						<TCEforms>
						<label>minOpacity:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0.4</default>
						</config>
						</TCEforms>
					</minOpacity>
					<maxOpacity>
						<TCEforms>
						<label>maxOpacity:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>1.0</default>
						</config>
						</TCEforms>
					</maxOpacity>
					<minScale>
						<TCEforms>
						<label>minScale:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0.4</default>
						</config>
						</TCEforms>
					</minScale>
					<maxScale>
						<TCEforms>
						<label>maxScale:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>1.0</default>
						</config>
						</TCEforms>
					</maxScale>
					<duration>
						<TCEforms>
						<label>Duration:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>600</default>
						</config>
						</TCEforms>
					</duration>
					<easing>
						<TCEforms>
						<label>easing:</label>
						<config>
							<type>select</type>
							<items type="array">
							  <numIndex index="0" type="array">
								<numIndex index="0">swing</numIndex>
								<numIndex index="1">swing</numIndex>
							  </numIndex>
							</items>
						</config>
						</TCEforms>
					</easing>
					<clickToFocus>
						<TCEforms>
						<label>clickToFocus:</label>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
						</TCEforms>
					</clickToFocus>
					<focusBearing>
						<TCEforms>
						<label>focusBearing:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0.0</default>
						</config>
						</TCEforms>
					</focusBearing>
					<debug>
						<TCEforms>
						<label>debug:</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</debug>
					<startingChild>
						<TCEforms>
						<label>startingChild:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0</default>
						</config>
						</TCEforms>
					</startingChild>
					<reflect>
						<TCEforms>
						<label>reflect:</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</reflect>
					<floatComparisonThreshold>
						<TCEforms>
						<label>floatComparisonThreshold:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>0.001</default>
						</config>
						</TCEforms>
					</floatComparisonThreshold>
					<autoplay>
						<TCEforms>
						<label>autoplay:</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</autoplay>
					<autoplayDuration>
						<TCEforms>
						<label>autoplayDuration:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>1000</default>
						</config>
						</TCEforms>
					</autoplayDuration>
					<autoplayPauseOnHover>
						<TCEforms>
						<label>autoplayPauseOnHover:</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</autoplayPauseOnHover>
					<enableDrag>
						<TCEforms>
						<label>enableDrag:</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</enableDrag>
					<dropDuration>
						<TCEforms>
						<label>dropDuration:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>600</default>
						</config>
						</TCEforms>
					</dropDuration>
					<dropAnimateTo>
						<TCEforms>
						<label>dropAnimateTo:</label>
						<config>
							<type>select</type>
							<items type="array">
							  <numIndex index="0" type="array">
								<numIndex index="0">next</numIndex>
								<numIndex index="1">next</numIndex>
							  </numIndex>
							  <numIndex index="1" type="array">
								<numIndex index="0">previous</numIndex>
								<numIndex index="1">previous</numIndex>
							  </numIndex>
							  <numIndex index="2" type="array">
								<numIndex index="0">nearest</numIndex>
								<numIndex index="1">nearest</numIndex>
							  </numIndex>
							</items>
							<default>nearest</default>
						</config>
						</TCEforms>
					</dropAnimateTo>
					<dragAxis>
						<TCEforms>
						<label>dragAxis:</label>
						<config>
							<type>select</type>
							<items type="array">
							  <numIndex index="0" type="array">
								<numIndex index="0">x</numIndex>
								<numIndex index="1">x</numIndex>
							  </numIndex>
							  <numIndex index="1" type="array">
								<numIndex index="0">y</numIndex>
								<numIndex index="1">y</numIndex>
							  </numIndex>         
							</items>
						</config>
						</TCEforms>
					</dragAxis>
					<dragFactor>
						<TCEforms>
						<label>dragFactor:</label>
						<config>
							<type>input</type>
							<size>3</size>
							<default>4</default>
						</config>
						</TCEforms>
					</dragFactor>
					<triggerFocusEvents>
						<TCEforms>
						<label>triggerFocusEvents:</label>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
						</TCEforms>
					</triggerFocusEvents>
					<triggerBlurEvents>
						<TCEforms>
						<label>triggerBlurEvents:</label>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
						</TCEforms>
					</triggerBlurEvents>
					<responsive>
						<TCEforms>
						<label>responsive:</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</responsive>
				</el>
			</ROOT>
		</s_roundabout>
		<s_javascript>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.sheet_jssettings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<noJQfile>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_nojq</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</noJQfile>
					<noRAfile>
						<TCEforms>
						<label>LLL:EXT:pwo_roundabout/locallang_db.xml:pi_flexform.field_nora</label>
						<config>
							<type>check</type>
						</config>
						</TCEforms>
					</noRAfile>
				</el>
			</ROOT>
		</s_javascript>
	</sheets>
</T3DataStructure>